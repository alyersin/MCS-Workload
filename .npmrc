# NPM CONFIGURATION FOR VERCEL BUILDS
# DISABLE INTEGRITY CHECKS TO RESOLVE BUILD ERRORS
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-retries=5
fetch-retry-factor=2

# USE REGISTRY WITH BETTER RELIABILITY
registry=https://registry.npmjs.org/

# DISABLE PACKAGE-LOCK INTEGRITY CHECKS
package-lock=false

# CLEAR CACHE BEFORE INSTALL
cache-min=0
prefer-offline=false
